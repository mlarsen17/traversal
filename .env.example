# Dagster
DAGSTER_HOME=/opt/dagster/dagster_home
DAGSTER_GRPC_PORT=4000

# Dagster Postgres (docker mode)
DAGSTER_PG_HOST=dagster_postgres
DAGSTER_PG_PORT=5432
DAGSTER_PG_USER=dagster
DAGSTER_PG_PASSWORD=dagster
DAGSTER_PG_DB=dagster

# Metadata Postgres (docker mode)
METADATA_PG_HOST=metadata_postgres
METADATA_PG_PORT=5432
METADATA_PG_USER=metadata
METADATA_PG_PASSWORD=metadata
METADATA_PG_DB=metadata

# MinIO server bootstrap credentials (docker mode)
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=minioadmin

# Unified S3/MinIO runtime credentials
S3_ENDPOINT_URL=http://minio:9000
S3_ACCESS_KEY_ID=minioadmin
S3_SECRET_ACCESS_KEY=minioadmin
S3_REGION=us-east-1
S3_BUCKET=health-raw

# Dev mode overrides (non-docker)
# METADATA_DB_URL=sqlite:///./.local/metadata.db
# S3_ENDPOINT_URL=http://127.0.0.1:9000
